{
    "name": "zblues/ezdb",
    "description": "Codeigniter DB Helper",
    "keywords": [
        "ezdb"
    ],
    "homepage": "https://github.com/zblues/ezdb",
    "license": "MIT",
    "authors": [
        {
            "name": "Sanghoon Youn",
            "email": "sanghoon.youn@gmail.com",
            "homepage": "https://github.com/zblues",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : ">=5.3.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*"
    },
    "autoload": {
        "psr-4": {
            "Zblues\\Codeigniter\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Zblues\\Codeigniter\\Test\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
